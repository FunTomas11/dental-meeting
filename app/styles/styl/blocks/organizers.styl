.row_top
  display flex
  background white
  flex-direction column
  align-items center

.organizers_title
  font-family userfont
  color #1e3b00
  text-align center
  margin-top 50px
  padding-bottom 10px
  width 40%

.row_bottom
  display block
  justify-content center

.organizers_photo
  display flex
  justify-content center
  align-items center
  background #F8F8F8
  min-width 1200px
  min-height 700px
  margin 50px
  box-shadow 0 4px 8px 0 rgba(0, 0, 0, 0.01), 0 6px 20px 0 rgba(0, 0, 0, 0.10)

.coming_soon
  font-family userfont
  font-size 40px
  text-align justify
  color #525252

.ig_fb
  display inline-flex
  justify-content center
  align-items center
  margin-bottom 50px

.ig_link
  margin-right 2%

.fb_link
  margin-left 2%

.img_media
  font-size 110px
  color #1e3b00