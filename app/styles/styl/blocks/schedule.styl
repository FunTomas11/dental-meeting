.schedule
  display flex-box
  margin 10px
  width 100%

.schedule_info_meeting
  display flex

.schedule_info
  background ghostwhite
  padding 10px
  margin-right 150px
  margin-left 50px
  box-shadow 0 4px 8px 0 rgba(0, 0, 0, 0.01), 0 6px 20px 0 rgba(0, 0, 0, 0.10)

.meeting_venues
  display block
  background ghostwhite
  padding 10px
  margin-right 50px
  box-shadow 0 4px 8px 0 rgba(0, 0, 0, 0.01), 0 6px 20px 0 rgba(0, 0, 0, 0.10)

.meeting_venues_title
  font-size 35px
  margin-left 15px
  font-family Teko, sans-serif

.img
  margin 5px
  border-radius 15px

.img_link
  display grid
  justify-items center
  margin-left 15px
  margin-right 15px

.meeting_venues_hotel
  display flex
  wrap-option flex

.workshop_row
  display flex

.workshop
  margin-left 80px
  margin-top 45px
  background #0d6efd
  max-width 700px
  width 100%
  font-family accent-font
  min-height 100px


.social_program_div
  display flex
  background darkgrey
  justify-items right
  align-items right
.social_program
  margin-left 80px
  margin-top 45px
  background #0d6efd
  max-width 700px
  width 100%
  min-height 100px